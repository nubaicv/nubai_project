{# templates/product_list.html.twig #}

{% extends 'base.html.twig' %}

{% block title %}
    {{ title }}
{% endblock %}


{% block body %}
    <div class="center">
        <main>
            {% for message in app.flashes('success') %}
                <div>{{ message }}</div>
            {% endfor %}
            <h1>{{ title }}</h1>
            <hr>
        </main>
    </div>
{% endblock %}
